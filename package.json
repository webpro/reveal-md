{
  "name": "reveal-md",
  "description": "reveal.js on steroids! Get beautiful reveal.js presentations from your Markdown files.",
  "keywords": [
    "markdown",
    "reveal",
    "revealjs",
    "html",
    "server",
    "cli"
  ],
  "version": "5.5.2",
  "repository": {
    "type": "git",
    "url": "git://github.com/webpro/reveal-md.git"
  },
  "homepage": "https://github.com/webpro/reveal-md#readme",
  "bugs": "https://github.com/webpro/reveal-md/issues",
  "license": "MIT",
  "bin": {
    "reveal-md": "bin/reveal-md.js"
  },
  "files": [
    "bin",
    "lib"
  ],
  "type": "module",
  "scripts": {
    "test": "node --test test/*.spec.js",
    "lint": "eslint lib test",
    "format": "prettier --write lib/**/*.js bin/*.js test/*.js *.md",
    "release": "release-it"
  },
  "dependencies": {
    "debug": "4.3.4",
    "express": "4.18.2",
    "fs-extra": "11.1.1",
    "glob": "10.3.10",
    "highlight.js": "11.9.0",
    "livereload": "0.9.3",
    "lodash": "4.17.21",
    "mustache": "4.2.0",
    "open": "9.1.0",
    "reveal.js": "4.5.0",
    "serve-favicon": "2.5.0",
    "update-notifier": "7.0.0",
    "yaml-front-matter": "4.1.1",
    "yargs-parser": "21.1.1"
  },
  "optionalDependencies": {
    "puppeteer": "21.5.0"
  },
  "devDependencies": {
    "eslint": "8.52.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-import": "2.29.0",
    "eslint-plugin-prettier": "5.0.1",
    "prettier": "3.0.3",
    "release-it": "16.2.1"
  },
  "engines": {
    "node": ">=20"
  }
}
