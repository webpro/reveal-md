{
  "name": "reveal-md",
  "description": "reveal.js on steroids! Get beautiful reveal.js presentations from your Markdown files.",
  "keywords": [
    "markdown",
    "reveal",
    "revealjs",
    "html",
    "server",
    "cli"
  ],
  "version": "5.3.1",
  "repository": {
    "type": "git",
    "url": "git://github.com/webpro/reveal-md.git"
  },
  "homepage": "https://github.com/webpro/reveal-md#readme",
  "bugs": "https://github.com/webpro/reveal-md/issues",
  "license": "MIT",
  "bin": {
    "reveal-md": "bin/reveal-md.js"
  },
  "files": [
    "bin",
    "lib"
  ],
  "scripts": {
    "test": "bron test/*.spec.js",
    "lint": "eslint lib test",
    "format": "prettier --write lib/**/*.js bin/*.js test/*.js *.md",
    "release": "release-it"
  },
  "dependencies": {
    "debug": "4.3.3",
    "express": "4.17.1",
    "fs-extra": "10.0.0",
    "glob": "7.2.0",
    "highlight.js": "10.7.2",
    "livereload": "0.9.3",
    "lodash": "4.17.21",
    "mustache": "4.2.0",
    "open": "8.4.0",
    "reveal.js": "4.2.1",
    "serve-favicon": "2.5.0",
    "try-require": "1.2.1",
    "update-notifier": "5.1.0",
    "yaml-front-matter": "4.1.1",
    "yargs-parser": "20.2.9"
  },
  "optionalDependencies": {
    "puppeteer": "1.19.0"
  },
  "devDependencies": {
    "bron": "1.1.1",
    "eslint": "8.4.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-prettier": "4.0.0",
    "prettier": "2.5.1",
    "release-it": "14.11.8"
  },
  "engines": {
    "node": ">=10"
  }
}
